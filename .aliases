alias aa="c ~/Desktop/aa"
alias ..="c .."
alias ...="c ../.."
alias l="ls"
alias a="atom"
alias bp="atom ~/.bashrc"
alias ds="c ~/Desktop"
alias doc="c ~/Documents"
alias dl="c ~/Downloads"
alias t="touch"
